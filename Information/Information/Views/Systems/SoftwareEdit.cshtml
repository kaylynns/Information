@model Entity.info_Software

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>软件名称修改</title>
</head>
<body>
    <h2 align="center">软件名称修改</h2>
    @using (Html.BeginForm(" SoftwareEdits", "System", "post"))
    {
        <table align="center">
            <tr>
                <td>软件名称</td>
                <td>
                    @Html.HiddenFor(model => model.SID)
                @Html.EditorFor(model => model.Sdynacomm)</td>
            </tr>
            <tr>
                <td>备注</td>
                <td>@Html.EditorFor(model => model.Sremark)</td>
            </tr>
            <tr>
                <td colspan="2" align="center">
                    <input type="submit" value="更改" Class="btn1" style="margin-left:10px" />
                    <input type="button" value="返回" Class="btn1" style="margin-left:10px" OnClick="window.location.href = '/Systems/ SoftwareSelect'" />

                </td>

            </tr>
        </table>


    }
</body>

</html>
